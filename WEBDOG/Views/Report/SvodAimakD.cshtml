@model IEnumerable<WEBDOG.Data.ViewSvodAimaksD>

@{
    ViewData["Title"] = "SvodAimakD";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrdName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.kolSobak)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Expr1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QvarK1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QvarK2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QvarK3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QvarK4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KolDos)
            </th>            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrdName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kolSobak)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expr1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QvarK1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QvarK2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QvarK3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QvarK4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KolDos)
            </td>

        </tr>
}
    </tbody>
</table>
